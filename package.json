{
  "name": "ts-template",
  "version": "1.0.0",
  "main": "src/server.ts",
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src --write",
    "format": "biome format src --write",
    "check": "biome check src",
    "test": "dotenv -e .env.test vitest run",
    "test:watch": "dotenv -e .env.test vitest",
    "test:e2e": "dotenv -e .env.test vitest run test/*.e2e.spec.ts",
    "test:integration": "dotenv -e .env.test vitest run test/*.integration.spec.ts",
    "test:coverage": "dotenv -e .env.test vitest run --coverage",
    "build": "tsup",
    "docs:generate": "pnpm run build && ts-node scripts/generate-openapi.ts",
    "docs:preview": "pnpm dlx serve docs",
    "prepare": "lefthook install",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/ioredis-mock": "^8.2.6",
    "@types/node": "^22.15.19",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.4",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "ioredis-mock": "^8.9.0",
    "lint-staged": "^16.1.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@duqueuniverso/biome-config": "^1.3.2",
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@libsql/client": "^0.15.9",
    "drizzle-orm": "^0.44.2",
    "fastify": "^5.3.3",
    "ioredis": "^5.6.1",
    "pg": "^8.16.0",
    "zod": "^3.25.17"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
